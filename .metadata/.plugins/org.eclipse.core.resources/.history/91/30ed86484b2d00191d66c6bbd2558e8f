spring:
  application:
    name: zuul-service
server:
  port: 8903
  
eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
      defaultZone: http://localhost:8761/eureka/
      
zuul:
  routes:
    server:
      path: /server/**
      service-id: hello-server
    client:
      path: /client/**
      service-id: hello-client
  